{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "Build",
        "type": "shell",
        "command": "npm run build",
        "group": "build"
      },
      {
        "label": "Start",
        "type": "shell",
        "command": "yarn start",
        "group": "none",
        "dependsOn": ["Build"]
      },
      {
        "label": "Deploy",
        "type": "shell",
        "command": "npx gh-pages -d build",
        "group": "none",
        "dependsOn": ["Build"]
      }
    ]
  }
  